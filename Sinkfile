app:
  name: product-service
  listeners: 
  - 8080
  - 8081
  routes:
  - path: /
    methods: ['GET', 'POST', 'DELETE', 'PATCH', 'PUT', 'OPTIONS'] 
    match: prefix
    sink: v1
  sinks:
  - name: v1
    strategy: 'random'
    upstreams:
    - address: 'localhost'
      port: 9000
      weight: 70
    - address: 'localhost'
      port: 9001
      weight: 150
